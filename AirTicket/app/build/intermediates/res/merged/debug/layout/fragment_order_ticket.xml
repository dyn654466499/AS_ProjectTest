<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.daemon.fragments.OrderTicketFragment">

    <ScrollView
        android:id="@+id/sv_my_consume_ticket"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            >
            <View
                android:layout_width="match_parent"
                android:layout_height="15dp"
                android:background="@null"
                />
        <!-- TODO: Update blank fragment layout -->
        <com.daemon.customview.ListViewForScrollView
            android:id="@+id/lv_my_consume_order_ticket"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:dividerHeight="15dp"
            android:divider="@color/ticket_bg_gray"
            android:footerDividersEnabled="true"
            android:headerDividersEnabled="true"
            />
            <View
                android:layout_width="match_parent"
                android:layout_height="15dp"
                android:background="@null"
                />
        </LinearLayout>
    </ScrollView>

</FrameLayout>
